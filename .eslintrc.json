{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1
      }
    ],
    "import/newline-after-import": [
      "warn",
      {
        "count": 1
      }
    ],
    "quotes": ["error", "single"],
    "semi": [
      "error",
      "always",
      {
        "omitLastInOneLineBlock": true
      }
    ]
  }
}
